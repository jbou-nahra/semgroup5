@startuml

actor DA as "Data Analyst"

rectangle Database

rectangle "World Population Data System" {
    usecase UC1 as "Get The population of people, people living in cities, and people not living in cities in each region. "

    usecase UCa as "Print countries  "

    DA - UC1
    UC1 ..> UCa : include
    UC1 - Database
}
@enduml